<?xml version="1.0"?>
<launch>

	<arg name="robot_ns" />
	<arg name="world_path" default="worlds/empty.world"/>

	<include file="$(find agvmecanumwheel_gazebo)/launch/agv.launch">
		<arg name="robot_ns" value="agv1"/>	
		<arg name="x" value="3.30"/>
		<arg name="y" value="2.5"/>
		<arg name="yaw" value="1.57"/>
	</include>



	<include file="$(find agvmecanumwheel_gazebo)/launch/start_gazebo.launch" >
		<arg name="world_path" value="$(arg world_path)"/>
	</include>

</launch>

